   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "#CALCULATE ALL THE COMPONENTS BASED ON THE ABOVE FORMULA\n",
    "v = movies_cleaned_df['vote_count']\n",
    "R = movies_cleaned_df['vote_average']\n",
    "C = movies_cleaned_df['vote_average'].mean()\n",
    "m = movies_cleaned_df['vote_count'].quantile(0.70)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [],
   "source": [
    "movies_cleaned_df['weighted_average'] = ((R*v)+((C*m)))/(v+m)"
   ]
  },
